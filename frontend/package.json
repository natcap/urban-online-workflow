    "version": "0.0.0",
    "scripts": {
        "start": "vite",
        "start-docker-prod": "cross-env MODE=docker VITE_DEVMODE=false vite --host 0.0.0.0 --port 3000",
        "start-docker-dev": "cross-env MODE=docker VITE_DEVMODE=true vite --host 0.0.0.0 --port 3000",
