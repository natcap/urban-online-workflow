        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@vitejs/plugin-react": "^4.0.1",
        "@vitest/coverage-v8": "^0.32.2",
        "c8": "^7.11.3",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0 || ^8.2.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
